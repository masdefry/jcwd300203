{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@prisma/client": ["node_modules/@prisma/client"],
    },
    "typeRoots": ["./node_modules/@types", "./src/types"]
  },
  // "include": ["src/**/*"],
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
